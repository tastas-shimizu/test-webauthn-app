{
  "name": "test-webauthn-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next-dev": "next dev",
    "ssl-proxy": "local-ssl-proxy --key localhost-key.pem --cert localhost.pem --source 3001 --target 3000",
    "dev": "concurrently \"npm run next-dev\" \"npm run ssl-proxy\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "prisma:migrate:local": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:migrate:supabase": "dotenv -e .env.supabase -- prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "local-ssl-proxy": "^2.0.5",
    "prisma": "^6.8.2",
    "supabase": "^2.26.9",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
